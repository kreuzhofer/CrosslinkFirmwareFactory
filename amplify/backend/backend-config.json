{
  "api": {
    "marlinbuildopsapi": {
      "service": "AppSync",
      "providerPlugin": "awscloudformation",
      "output": {
        "authConfig": {
          "additionalAuthenticationProviders": [
            {
              "authenticationType": "AWS_IAM"
            }
          ],
          "defaultAuthentication": {
            "authenticationType": "AMAZON_COGNITO_USER_POOLS",
            "userPoolConfig": {
              "userPoolId": "authmarlinbuildopsfc6385a7"
            }
          }
        }
      }
    },
    "patreonapi": {
      "service": "API Gateway",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "patreonwebhookapifunction",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ]
    }
  },
  "auth": {
    "marlinbuildopsfc6385a7": {
      "service": "Cognito",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "marlinbuildopsfc6385a7PreSignup",
          "triggerProvider": "Cognito",
          "attributes": [
            "Arn",
            "Name"
          ]
        },
        {
          "category": "function",
          "resourceName": "marlinbuildopsfc6385a7PreAuthentication",
          "triggerProvider": "Cognito",
          "attributes": [
            "Arn",
            "Name"
          ]
        },
        {
          "category": "function",
          "resourceName": "marlinbuildopsfc6385a7PostConfirmation",
          "triggerProvider": "Cognito",
          "attributes": [
            "Arn",
            "Name"
          ]
        },
        {
          "category": "function",
          "resourceName": "marlinbuildopsfc6385a7PreTokenGeneration",
          "triggerProvider": "Cognito",
          "attributes": [
            "Arn",
            "Name"
          ]
        }
      ],
      "customAuth": false
    },
    "userPoolGroups": {
      "service": "Cognito-UserPool-Groups",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "auth",
          "resourceName": "marlinbuildopsfc6385a7",
          "attributes": [
            "UserPoolId",
            "AppClientIDWeb",
            "AppClientID",
            "IdentityPoolId"
          ]
        }
      ]
    }
  },
  "hosting": {
    "amplifyhosting": {
      "service": "amplifyhosting",
      "providerPlugin": "awscloudformation",
      "type": "manual"
    }
  },
  "function": {
    "marlinbuildopsfc6385a7PreSignup": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "api",
          "resourceName": "marlinbuildopsapi",
          "attributes": [
            "GraphQLAPIIdOutput"
          ]
        }
      ] 
    },
    "AddBuildDefinitionFromWebHook": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "api",
          "resourceName": "marlinbuildopsapi",
          "attributes": [
            "GraphQLAPIIdOutput",
            "GraphQLAPIEndpointOutput"
          ]
        },
        {
          "category": "auth",
          "resourceName": "marlinbuildopsfc6385a7",
          "attributes": [
            "UserPoolId"
          ]
        }
      ]
    },
    "patreonwebhookapifunction": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": []
    },
    "marlinbuildopsfc6385a7PreAuthentication": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "api",
          "resourceName": "marlinbuildopsapi",
          "attributes": [
            "GraphQLAPIIdOutput"
          ]
        }
      ] 
    },
    "marlinbuildopsfc6385a7PostConfirmation": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    },
    "marlinbuildopsfc6385a7PreTokenGeneration": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "api",
          "resourceName": "marlinbuildopsapi",
          "attributes": [
            "GraphQLAPIIdOutput"
          ]
        }
      ]      
    }
  },
  "storage": {
    "userdata": {
      "service": "S3",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "auth",
          "resourceName": "marlinbuildopsfc6385a7",
          "attributes": [
            "UserPoolId"
          ]
        },
        {
          "category": "auth",
          "resourceName": "userPoolGroups",
          "attributes": [
            "EveryoneGroupRole"
          ]
        },
        {
          "category": "auth",
          "resourceName": "userPoolGroups",
          "attributes": [
            "AdminGroupRole"
          ]
        }
      ]
    }
  },
  "customresources": {
    "iamroles": {
      "service": "IamRoles",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "auth",
          "resourceName": "userPoolGroups",
          "attributes": [
            "EveryoneGroupRole"
          ]
        },				
        {
          "category": "auth",
          "resourceName": "userPoolGroups",
          "attributes": [
            "AdminGroupRole"
          ]
        }
      ]
    }
  }
}